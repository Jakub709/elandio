                  </div>
                  <div>
                    <input type="checkbox" id="jihoceskyKraj" value="true" />
                    <label for="jihoceskyKraj">Jihočeský kraj</label>
                  </div>
                  <div>
                    <input type="checkbox" id="stredoceskyKraj" value="true" />
                    <label for="stredoceskyKraj">Středočeský kraj</label>
                  </div>
                  <div>
                    <input
                      type="checkbox"
                      id="kralovehradeckyKraj"
                      value="true"
                    />
                    <label for="kralovehradeckyKraj"
                      >Královehradecký kraj</label
                    >
                  </div>

                  <div>
                    <input type="checkbox" id="jihomoravskyKraj" value="true" />
                    <label for="jihomoravskyKraj">Jihomoravský kraj</label>
                  </div>
                  <div>
                    <input
                      type="checkbox"
                      id="moravskoslezskyKraj"
                      value="true"
                    />
                    <label for="moravskoslezskyKraj"
                      >Moravskoslezský kraj</label
                    >
                  </div>
                  <div>
                    <input type="checkbox" id="zlinskyKraj" value="true" />
                    <label for="zlinskyKraj">Zlínský kraj</label>
                  </div>
                  <div>
                    <input type="checkbox" id="olomouckyKraj" value="true" />
                    <label for="olomouckyKraj">Olomoucký kraj</label>
                  </div>
                  <div>
                    <input type="checkbox" id="praha" value="true" />
                    <label for="praha">Praha</label>
                  </div>
                  <div>
                    <input type="checkbox" id="ostrava" value="true" />
                    <label for="ostrava">Ostrava</label>
                  </div>
                  <div>
                    <input type="checkbox" id="brno" value="true" />
                    <label for="brno">Brno</label>
                  </div>
                </div>
              </div>
            </div>
            <input class="button-1" type="submit" value="Vyhledat" />
          </form>
        </div>
      </div>
    </div>
