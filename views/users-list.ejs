      integrity="sha512-xodZBNTC5n17Xt2atTPuE1HxjVMSvLVW9ocqUKLsCC5CXdbqCmblAshOMAS6/keqq/sMZMZ19scR4PsZChSR7A=="
      crossorigin=""
    />
    <script
      src="https://unpkg.com/leaflet@1.7.1/dist/leaflet.js"
      integrity="sha512-XQoYMqMTK8LvdxXYG3nZ448hOEQiglfqkJs1NOQV44cWnUrBc8PkAOcXy20w0vlaXaVUearIOBhiXZ5V3ynxwA=="
      crossorigin=""
    ></script>
    <script src="https://unpkg.com/vue@next" defer></script>
    <!--========== CSS ==========-->
    <link
      rel="stylesheet"
      href="/css/general/generalInner.css
  "
    />
    <link
      rel="stylesheet"
      href="/css/usersList/usersList.css
    "
    />

    <!--========== VUE, JQUERY a SWEET ==========-->
    <script src="https://unpkg.com/vue@3/dist/vue.global.js"></script>
    <script
      src="https://code.jquery.com/jquery-3.5.1.min.js"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"
    ></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <title>Seznam uživatelů</title>
  </head>
  <body>
    <!--========== HEADER ==========-->

    <header class="header">
      <div class="header__container">
        <img
          src="images/elandio_logo.png"
          alt=""
          class="header__img__delete header__img"
        />

        <p class="header__logo">Uživatelé</p>

        <!-- <div class="header__search">
          <input type="search" placeholder="Hledat" class="header__input" />
          <i class="bx bx-search header__icon"></i>
          <div class="tooltip" id="modal-filter-button">
            <i class="bx bx-task header__icon header__icon__filter"></i>
            <span class="tooltiptext"
              >Klikněte kdekoliv na stránce 2x a objeví se filtr.</span
            >
          </div>
        </div> -->
        <div class="header__search">
          <input type="search" placeholder="Hledat" class="header__input" />
          <i class="bx bx-search header__icon"></i>
          <div class="tooltip" id="modal-filter-button">
            <i class="bx bx-slider-alt header__icon header__icon__filter"></i>
            <span class="tooltiptext">Filtr</span>
          </div>
        </div>

        <div class="header__toggle">
          <i class="bx bx-menu" id="header-toggle"></i>
        </div>
      </div>
    </header>

    <!--========== NAV ==========-->
    <div class="nav" id="navbar">
      <nav class="nav__container">
        <div>
          <a href="/posts-list" class="nav__link nav__logo">
            <i class="bx bx-world nav__icon"></i>
            <img
              src="/images/elandio_logo_sidebar.png"
              alt=""
              class="header__img"
            />
          </a>

          <div class="nav__list">
            <div class="nav__items">
              <h3 class="nav__subtitle">Příspěvky</h3>

              <a href="/posts-list" class="nav__link">
                <i class="bx bx-task nav__icon"></i>
                <span class="nav__name">Seznam příspěvků</span>
              </a>

              <a href="/my-posts" class="nav__link">
                <i class="bx bx-pencil nav__icon"></i>
                <span class="nav__name">Moje příspěvky</span>
              </a>
            </div>

            <div class="nav__items">
              <h3 class="nav__subtitle">Uživatelé</h3>

              <a href="/users-list" class="nav__link">
                <i class="bx bx-user nav__icon"></i>
                <span class="nav__name">Seznam uživatelů</span>
              </a>
              <div class="nav__dropdown">
                <div class="nav__link">
                  <i class="bx bx-cog nav__icon"></i>
                  <span class="nav__name">Můj profil</span>
                  <i
                    class="bx bx-chevron-down nav__icon nav__dropdown-icon"
                  ></i>
                </div>

                <div class="nav__dropdown-collapse">
                  <div class="nav__dropdown-content">
                    <a href="/my-profile" class="nav__dropdown-item"
                      >Editovat profil</a
                    >
                    <a href="/my-profile-watch" class="nav__dropdown-item"
                      >Zobrazit profil</a
                    >
                    <a href="/bank" class="nav__dropdown-item">Educoiny</a>
                  </div>
                </div>
              </div>
            </div>

            <div class="nav__items">
              <a href="/notifications" class="nav__link">
                <i class="bx bx-bell nav__icon"></i>
                <span class="nav__name">Oznámení</span>
              </a>
              <a href="/messages" class="nav__link">
                <i class="bx bx-message-rounded nav__icon"></i>
                <span class="nav__name">Zprávy</span>
                <span id="messagesBadge"></span>
              </a>
            </div>
          </div>
        </div>

        <a href="/logout" class="nav__link nav__logout">
          <i class="bx bx-log-out nav__icon"></i>
          <span class="nav__name">Odhlásit se</span>
        </a>
      </nav>
    </div>

    <!--========== CONTENTS ==========-->
    <main>
      <div id="app">
        <div id="left">
          <div class="users">
            <div class="container">
              <% users.forEach(function(user) { %>
              <div
                class="user-profile col-12 col-sm-6 col-md-4 col-lg-3"
                @click="seeProfile(`<%= user._id %>`)"
              >
                <div class="user-info">
                  <div class="picture <%= user.facultyNameCSS %>">
                    <img class="img-fluid" src="<%= user.profilePic %>" />
                  </div>
                  <div class="user-content">
                    <h3 class="name"><%= user.name %></h3>

                    <h3 class="username">@<%= user.username %></h3>

                    <h4 class="title">
                      <% if(user.fieldOfStudy == ""){%> Nový uživatel <%}else{%>
                      <%=user.fieldOfStudy%> <%}%>
                    </h4>
                  </div>
                  <ul class="social">
                    <li>
                      <a href="/user-profile/<%= user._id %>" aria-hidden="true"
                        ><b><%=user.followersCounter%></b> sledujících</a
                      >
                    </li>
                    <li>
                      <a href="/user-profile/<%= user._id %>" aria-hidden="true"
                        ><b><%=user.followingCounter%></b> sleduje</a
                      >
                    </li>
                  </ul>
                </div>
              </div>
              <% }); %>
            </div>
            <div class="no-results">
              <% if(usersCounter<=0){ %>
              <img
                src="data:image/png;base64,"
                alt=""
              />

              <p>Žádné uživatele jsme nenašli.</p>
              <% } else{ %>
              <p></p>
              <% } %>
            </div>
            <div class="next-previous">
              <!-- url =  /posts-list?&type=nabídka-->
              <a href="<%= url %>&page=<%= currentPage-1 %>"
                ><%= textPrevious %></a
              >
              <a href="<%= url %>&page=<%= currentPage+1 %>"><%= textNext %></a>
            </div>
          </div>
        </div>
        <div id="right">
          <div class="sidebar">
            <div v-if="profileSelected == false" class="card-container">
              <span class="pro"><%=user.faculty%></span>
              <img class="round" src="<%=user.profilePic%>" alt="user" />
              <h3><%=user.name%></h3>
              <h6><%=user.accountBalance%> EC</h6>
              <p><%=user.fieldOfStudy%><br /></p>
              <div class="buttons">
                <button
                  onclick="location.href='/my-profile-watch'"
                  class="primary ghost"
                >
                  Profil
                </button>
                <button onclick="location.href='/my-posts'" class="primary">
                  Příspěvky
                </button>
              </div>
              <div class="profile-stats">
                <ul>
                  <li>
                    <span class="profile-stat-count"
                      ><b><%=user.postsCounter%></b></span
                    >
                    příspěvků
                  </li>
                  <li>
                    <span class="profile-stat-count"
                      ><b><%=user.followersCounter%></b></span
                    >
                    sledující
                  </li>
                  <li>
                    <span class="profile-stat-count"
                      ><b><%=user.followingCounter%></b></span
                    >
                    sledovaní
                  </li>
                </ul>
              </div>
              <div class="skills">
                <h6>O mně</h6>
                <p class="about"><%=user.about%></p>
              </div>
            </div>
            <div v-if="profileSelected == true" class="card-container">
              <span class="pro">{{ faculty }}</span>
              <img class="round" v-bind:src="profilePic" alt="user" />
              <h3>{{ name }}</h3>

              <p>{{ fieldOfStudy }}<br /></p>
              <div class="buttons">
                <button v-bind:onclick="profilePage" class="primary ghost">
                  Profil
                </button>
                <button v-bind:onclick="sendMessage" class="primary">
                  Zpráva
                </button>
                <button
                  v-bind:onclick="sendNotificationBigScreen"
                  class="primary"
                >
                  ✋
                </button>
              </div>
              <div class="profile-stats">
                <ul>
                  <li>
                    <span class="profile-stat-count"
                      ><b>{{ posts }}</b></span
                    >
                    příspěvků
                  </li>
                  <li>
                    <span class="profile-stat-count"
                      ><b>{{ followers }}</b></span
                    >
                    sledující
                  </li>
                  <li>
                    <span class="profile-stat-count"
                      ><b>{{ following }}</b></span
                    >
                    sledovaní
                  </li>
                </ul>
              </div>
              <div class="skills">
                <h6>O mně</h6>
                <p class="about">
                  {{ about }}
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </main>
    <div>
      <!-- The Modal -->
