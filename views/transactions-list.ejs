<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!--========== BOX ICONS ==========-->
    <link rel="icon" href="/images/favicon.png" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css"
    />

    <!--========== CSS ==========-->
    <link
      rel="stylesheet"
      href="/css/general/generalInner.css
  "
    />
    <link
      rel="stylesheet"
      href="/css/bank/bank.css
    "
    />

    <!--========== JQUERY a SWEET ==========-->
    <script
      src="https://code.jquery.com/jquery-3.5.1.min.js"
      integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
      crossorigin="anonymous"
    ></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <title>Banka</title>
  </head>
  <body>
    <!--========== HEADER ==========-->
    <header class="header">
      <div class="header__container">
        <img src="images/elandio_logo.png" alt="" class="header__img" />

        <p class="header__logo">Banka</p>
        <p class="header__logo__second">
          <i>„Budoucnost bankovnictví.“ </i>
        </p>

        <div class="header__toggle">
          <i class="bx bx-menu" id="header-toggle"></i>
        </div>
      </div>
    </header>

    <!--========== NAV ==========-->
    <div class="nav" id="navbar">
      <nav class="nav__container">
        <div>
          <a href="/admin-secret-page" class="nav__link nav__logo">
            <i class="bx bx-world nav__icon"></i>
            <img
              src="/images/elandio_logo_sidebar.png"
              alt=""
              class="header__img"
            />
          </a>

          <div class="nav__list">
            <div class="nav__items">
              <a href="/admin-secret-page" class="nav__link">
                <i class="bx bx-task nav__icon"></i>
                <span class="nav__name">Dashboard</span>
              </a>
              <a href="/users-list" class="nav__link">
                <i class="bx bx-user nav__icon"></i>
                <span class="nav__name">Klienti</span>
              </a>
              <a href="/transactions-list" class="nav__link">
                <i class="bx bx-coin nav__icon"></i>
                <span class="nav__name">Transakce</span>
              </a>
            </div>
          </div>
        </div>

        <a href="/logout" class="nav__link nav__logout">
          <i class="bx bx-log-out nav__icon"></i>
          <span class="nav__name">Odhlásit se</span>
        </a>
      </nav>
    </div>

    <!--========== CONTENTS ==========-->
    <main>
      <div class="my-bank-page">
        <div class="container">
          <section class="section-movements">
            <h2 class="heading-secondary">Souhrn všech bankovních transakcí</h2>
            <p class="label-text date"></p>
            <div class="container-movements">
              <ul class="responsive-table">
                <li class="table-header">
                  <div class="col col-1">ID</div>
                  <div class="col col-2">Odesílatel</div>
                  <div class="col col-3">Částka</div>
                  <div class="col col-4">Příjemce</div>
                  <div class="col col-5">Datum</div>
                </li>
                <div class="movements"></div>
              </ul>
            </div>
          </section>
        </div>
      </div>
    </main>

    <!--========== MAIN JS ==========-->
    <script src="/js/transactionsList.js"></script>
    <script src="/js/commonLoggedIn.js"></script>
  </body>
</html>
